{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite",
    "build": "tsc && vite build && echo '<%@ page session=\"false\" %> ' | cat - ./dist/index.html > temp && mv temp ./dist/index.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"src/components/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .prettierignore",
    "test": "vitest run --passWithNoTests --bail 1 "
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.6.7",
    "axios-mock-adapter": "^1.22.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.4.1",
    "history": "^5.3.0",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.7",
    "react-bootstrap": "^2.9.2",
    "react-datepicker": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.3.1",
    "react-router-dom": "^6.15.0",
    "vite-plugin-html-env": "^1.2.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.15",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.4.5",
    "vitest": "^1.2.2"
  }
}
